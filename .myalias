alias gd='gcc -ansi -pedantic-errors -Wall -Wextra -g'
alias gc='gcc -ansi -pedantic-errors -Wall -Wextra -DNDEBUG -O3'
alias gd9='gcc -std=c99 -pedantic-errors -Wall -Wextra -g'
alias gc9='gcc -std=c99 -pedantic-errors -Wall -Wextra -DNDEBUG -O3'
alias vlg='valgrind --leak-check=yes --track-origins=yes'
alias vlgd='vlg build/./${PWD##*/}_debug'
alias vlgr='vlg build/./${PWD##*/}_rc'
alias cgdbd='cgdb build/./${PWD##*/}_debug'
alias cgdbr='cgdb build/./${PWD##*/}_rc'
alias ds='cd $HOME/git/ds'
alias debug='make -f ${PWD##*/}.mk debug'
alias clean='make -f ${PWD##*/}.mk clean'
alias rc='make -f ${PWD##*/}.mk release'
alias outd='build/./${PWD##*/}_debug'
alias outrc='build/./${PWD##*/}_rc'